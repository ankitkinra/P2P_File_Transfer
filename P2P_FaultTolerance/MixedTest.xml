<testsuite>
	<rounds>
		<round>
			<name>MixedTestCases</name>
			<operations>
				<operation>
					<sno>1</sno>
					<!-- getFileList would need to hit the server which it will get from command line -->
					<name>getFileList</name>
				</operation>
				<operation>
					<sno>2</sno>
					<!-- launchPeers would launch so many peers -->
					<name>launchPeers</name>
					<params>
						<entry>
							<string>numberOfPeers</string>
							<string>10</string>
						</entry>
					</params>
				</operation>
				<operation>
					<sno>3</sno>
					<!--  this will download a random file on a random peer -->
					<name>downloadFile</name>
					<repeatations>10</repeatations>
				</operation>
				<operation>
					<sno>4</sno>
					<!--  this will download a particular file on a random/particular peer -->
					<name>downloadFileNode</name>
					<params>
						<entry>
							<string>machineId</string>
							<string>1</string>
						</entry>
						<entry>
							<string>file</string>
							<string>test1.txt</string>
						</entry>
						<entry>
							<string>file</string>
							<string>test2.txt</string>
						</entry>
						<entry>
							<string>file</string>
							<string>test3.txt</string>
						</entry>
					</params>
				</operation>
				<operation>
					<sno>3</sno>
					<name>readItem</name>
					<params>
						<entry>
							<string>id</string>
							<string>2</string>
						</entry>

					</params>
				</operation>
			</operations>
		</round>
	</rounds>
</testsuite>